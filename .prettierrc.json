{
  "endOfLine": "lf",
  "useTabs": false,
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "semi": false,
  "importOrder": [
    "^@/env.mjs$",
    "^react",
    "^@[^/]",
    "^@/components/(.*)$",
    "^@/*/(.*)$",
    "^/*/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
